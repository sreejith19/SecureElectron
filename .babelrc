{
  "presets": [
    ["@babel/preset-env", {
      "targets": {
        "node": "10"
      }
    }],
    ["@babel/preset-typescript"]
  ],
  "sourceType": "unambiguous",
  "plugins": [
    ["@babel/plugin-proposal-class-properties", { "loose": false }],
    ["@babel/plugin-proposal-nullish-coalescing-operator"]
  ]
}